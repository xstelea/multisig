{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "useTabs": false,
  "tabWidth": 2,
  "printWidth": 80,
  "singleQuote": false,
  "trailingComma": "es5",
  "semi": true,
  "arrowParens": "always",
  "sortPackageJson": false,
  "ignorePatterns": [
    "multisig-server/target",
    ".repos",
    ".output",
    "*.lock",
    "*.gen.ts"
  ]
}
